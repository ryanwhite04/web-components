{
  "name": "ace-editor",
  "version": "1.0.0",
  "description": "Ace Editor Webcomponent built with Lit-Element",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "browser": "index.js",
  "homepage": "https://gitlab.com/ryanwhite04/ace-editor",
